@model ManagementArsip.Models.Lokasi

@{
    ViewData["Title"] = "Create";
}

<h1>Tambahkan Lokasi</h1>

<h4>Lokasi</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="IdLokasi" class="control-label">ID Lokasi</label>
                <input asp-for="IdLokasi" class="form-control" />
                <span asp-validation-for="IdLokasi" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NamaLokasi" class="control-label">Nama Lokasi</label>
                <input asp-for="NamaLokasi" class="form-control" />
                <span asp-validation-for="NamaLokasi" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IdUser" class="control-label">ID User</label>
                <input asp-for="IdUser" class="form-control" />
                <span asp-validation-for="IdUser" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
