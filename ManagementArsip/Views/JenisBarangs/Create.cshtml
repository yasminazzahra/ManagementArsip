@model ManagementArsip.Models.JenisBarang

@{
    ViewData["Title"] = "Create";
}

<h1>Tambahkan Jenis Barang</h1>

<h4>Jenis Barang</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="IdJenisbarang" class="control-label">ID Jenis Barang</label>
                <input asp-for="IdJenisbarang" class="form-control" />
                <span asp-validation-for="IdJenisbarang" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NamaJenisbarang" class="control-label">Nama Jenis Barang</label>
                <input asp-for="NamaJenisbarang" class="form-control" />
                <span asp-validation-for="NamaJenisbarang" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IdBarang" class="control-label">ID Barang</label>
                <input asp-for="IdBarang" class="form-control" />
                <span asp-validation-for="IdBarang" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
